{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-JMA-PROD"
		},
		"AzureBlobStorageLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageLS'"
		},
		"LS_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AzureBlobStorage'"
		},
		"LS_Snowflake_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_Snowflake'"
		},
		"SnowflakeV1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SnowflakeV1'"
		},
		"AzureKeyVaultForSnowflake_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://AzureKeyVaultManager.vault.azure.net/"
		},
		"LS_Snowflake_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "INVOICE"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-JMA-PROD_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageLS_connectionString": {
						"value": "[parameters('AzureBlobStorageLS_connectionString')]"
					},
					"LS_AzureBlobStorage_connectionString": {
						"value": "[parameters('LS_AzureBlobStorage_connectionString')]"
					},
					"LS_Snowflake_password": {
						"value": "[parameters('LS_Snowflake_password')]"
					},
					"SnowflakeV1_connectionString": {
						"value": "[parameters('SnowflakeV1_connectionString')]"
					},
					"AzureKeyVaultForSnowflake_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultForSnowflake_properties_typeProperties_baseUrl')]"
					},
					"LS_Snowflake_properties_typeProperties_database": {
						"value": "[parameters('LS_Snowflake_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}